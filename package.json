{
  "name": "vite-plugin-sk-router-guard",
  "description": "ðŸª A global routing guard plugin based on svelte-kit",
  "private": false,
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@8.7.0",
  "keywords": [
    "typescript",
    "javascript",
    "DX",
    "vue",
    "vue3",
    "vite",
    "react",
    "svelte",
    "web component"
  ],
  "license": "MIT",
  "author": "baiwusanyu-c",
  "homepage": "https://github.com/ikun-svelte",
  "repository": "https://github.com/ikun-svelte/vite-plugin-sk-router-guard",
  "bugs": "https://github.com/ikun-svelte/vite-plugin-sk-router-guard/issues",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts",
    "README.md",
    "README.ZH-CN.md"
  ],
  "engines": {
    "node": ">=v14"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "init": "pnpm i",
    "dev": "unbuild --stub",
    "play": "pnpm run --filter vite-plugin-sk-router-guard-playground dev",
    "build": "tsup",
    "release": "bumpp package.json --commit --push --tag",
    "publish:npm": "pnpm publish --no-git-checks --access public",
    "lint:fix": "eslint --fix ./ --ext .vue,.js,.ts,.jsx,.tsx,.json",
    "clean": "rimraf dist",
    "prepare": "npx simple-git-hooks",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:coverage": "vitest --coverage",
    "update:deps": "npx taze -w -r major && pnpm run init"
  },
  "peerDependencies": {
    "ansi-colors": "^4.1.3",
    "baiwusanyu-utils": "^1.0.14",
    "magic-string": "^0.30.0",
    "svelte": "^4.0.5",
    "magic-string-ast": "^0.3.0"
  },
  "dependencies": {
    "baiwusanyu-utils": "^1.0.15",
    "magic-string": "^0.30.3",
    "magic-string-ast": "^0.3.0",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@sveltejs/eslint-config": "^6.0.4",
    "@types/node": "^20.5.7",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "bumpp": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-svelte": "^2.33.0",
    "git-ensure": "^0.1.0",
    "lint-staged": "^14.0.1",
    "magic-string-ast": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "jsdom": "^22.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node verify-commit.js"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,svelte}": [
      "eslint ./ --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "plugin:svelte/prettier"
  }
}
